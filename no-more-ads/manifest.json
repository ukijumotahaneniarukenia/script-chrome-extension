{
  "manifest_version": 2,
  "author": "ukijumotahaneniarukenia",
  "homepage_url": "https://ukijumotahaneniarukenia.site/",
  "name": "no-more-ads",
  "description": "No More Ads",
  "version": "1.1",
  "icons": {
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*",
        "https://music.youtube.com/*"
      ],
      "js": [
        "suppressAds.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon_32.png",
    "default_title": "no-more-ads"
  }
}
