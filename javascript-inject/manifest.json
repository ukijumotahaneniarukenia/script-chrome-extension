{
  "manifest_version": 2,
  "author": "ukijumotahaneniarukenia",
  "homepage_url": "https://ukijumotahaneniarukenia.site/",
  "name": "injectJavascript",
  "description": "injectJavascript",
  "version": "1.1",
  "icons": {
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content_scripts.js"
      ]
    }
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon_32.png",
    "default_title": "injectJavascript"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ]
}
