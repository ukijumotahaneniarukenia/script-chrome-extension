{
  "manifest_version": 2,
  "author": "ukijumotahaneniarukenia",
  "homepage_url": "https://ukijumotahaneniarukenia.site/",
  "name": "vuetify-template",
  "description": "Download all XPATHs of the currently referenced web page and the DOM associated with them as a JSON file.",
  "version": "1.1",
  "icons": {
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "eventHandler.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon_32.png",
    "default_title": "vuetify-template",
    "default_popup": "display.html"
  },
  "content_security_policy": "script-src 'sha256-Dr1+ueaF5DuRHeBDta0+77aiL2XPJdk7SdwuX+R/xIE=' 'unsafe-eval' 'self' https://cdn.jsdelivr.net; object-src 'self'"
}
