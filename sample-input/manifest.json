{
  "manifest_version": 2,
  "author": "ukijumotahaneniarukenia",
  "homepage_url": "https://ukijumotahaneniarukenia.site/",
  "name": "sample-input",
  "description": "sample-input",
  "version": "1.0",
  "icons": {
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
