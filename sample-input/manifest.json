  "background":{
    "scripts":[
      "background.js"
    ],
    "persistent": false
  },
  "permissions": ["tabs", "history"],
  "browser_action": {
    "default_icon": "icon_32.png",
    "default_title": "sample-input",
    "default_popup": "display.html"
  }
}
