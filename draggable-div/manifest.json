{
  "manifest_version": 2,
  "author": "ukijumotahaneniarukenia",
  "homepage_url": "https://ukijumotahaneniarukenia.site/",
  "name": "draggable-div",
  "description": "draggable-div",
  "version": "1.0",
  "icons": {
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "permissions": ["tabs", "history"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["pageHandler.js", "messageHandler.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon_32.png",
    "default_title": "draggable-div",
    "default_popup": "display.html"
  }
}
